{
  "name": "mui-layout",
  "version": "1.0.0",
  "description": "A simple demo app for practice making a responsive dashboard / layout with Material UI and Next JS.",
  "main": "pages/index.js",
  "scripts": {
    "pretest": "yarn lint",
    "test": "yarn unit-test",
    "lint": "eslint . --ext .js,.jsx",
    "dev": "next",
    "build": "next build",
    "export": "yarn build && next export",
    "start": "next start",
    "unit-test": "NODE_ENV=test jest -c .jest-config.json --verbose false",
    "unit-test-w": "NODE_ENV=test jest -c .jest-config.json --watch",
    "storybook": "start-storybook -s ./ -p 9001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sjm-practice/mui-layout.git"
  },
  "keywords": [
    "react",
    "nextjs",
    "material-ui",
    "jest"
  ],
  "author": "Steve Marsh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sjm-practice/mui-layout/issues"
  },
  "homepage": "https://github.com/sjm-practice/mui-layout#readme",
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/styles": "^4.3.0",
    "babel-eslint": "^10.0.2",
    "clsx": "^1.0.4",
    "lodash": "^4.17.15",
    "next": "^9.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-backgrounds": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@testing-library/react": "^9.1.3",
    "babel-loader": "^8.0.6",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2"
  }
}
